{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "parser": "@babel/eslint-parser"
    },

    "rules": {
        "indent": [
            "error",
            4
        ],
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": "off"

      
    }

}
